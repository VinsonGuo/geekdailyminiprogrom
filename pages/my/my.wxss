.container{
  min-height: 100%;
  width: 100%;
  overflow: hidden;
  overflow-y: hidden
}
.userInfo{
  display: flex;
  flex-direction: column;
  /* padding: 50rpx; */
  align-items: center;
  background: #e85654;
  width: 100%;
  height: 300rpx;
}
.userinfo-avatar{
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx;
  border-radius: 50%
}
.userinfo-nickname{
  margin-top: 15rpx;
  color: #FFF
}
.my-item{
  width: 100%;
  padding: 15rpx 0;
  border-bottom: 1px solid #eee;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
}
.reload{
  width: 90%;
  color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}
