.container{
  min-height: 100%;
  width: 100%;
  overflow: hidden;
  overflow-y: hidden
}
.userInfo{
  display: flex;
  flex-direction: column;
  /* padding: 50rpx; */
  align-items: center;
  background: #33cd5f;
  width: 100%;
  height: 300rpx;
}
.userinfo-avatar{
  width: 160rpx;
  height: 160rpx;
  margin: 40rpx;
  border-radius: 50%
}
.userinfo-nickname{
  color: #FFF
}
.my-item{
  width: 100%;
  padding: 15rpx 0;
  border-bottom: 1px solid #eee;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
}
.reload{
  margin: 60rpx 16rpx;
}
